                            "schema": {
                                "$ref": "D:/test/limabean/task-manager/dto/schemas/json/authentication/registrationForm.json"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful registration",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
